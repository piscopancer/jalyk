{
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true,
    "**/dist/**": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true,
    "**/dist/**": true,
    "**/node_modules/**": true
  },
  "terminal.integrated.profiles.windows": {
    "_client": {
      "source": "PowerShell",
      "icon": "globe",
      "overrideName": true,
      "color": "terminal.ansiBlue",
      "args": ["-WorkingDirectory", "apps/client"]
    },
    "_server": {
      "source": "PowerShell",
      "icon": "server",
      "overrideName": true,
      "color": "terminal.ansiRed",
      "args": ["-WorkingDirectory", "apps/server"]
    },
    "_studio": {
      "source": "PowerShell",
      "icon": "home",
      "overrideName": true,
      "color": "terminal.ansiGreen",
      "args": ["-WorkingDirectory", "packages/studio"]
    }
  }
}
