{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "@jalyk/server#server:dev": {},
    "@jalyk/server#prisma:dev": {},
    "@repo/studio#dev:dts": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "with": ["@jalyk/server#server:dev", "@jalyk/server#prisma:dev", "@repo/studio#dev:dts"]
    }
  }
}
