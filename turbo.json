{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "@jalyk/server#dev": {},
    "@jalyk/server#db:dev": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "with": ["@jalyk/server#dev", "@jalyk/server#db:dev"]
    }
  }
}
