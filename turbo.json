{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["/dist/**"]
    },
    "@jalyk/server#server:dev": {},
    "@jalyk/server#prisma:dev": {},
    "dev": {
      "with": ["@jalyk/server#server:dev", "@jalyk/server#prisma:dev"],
      "cache": false,
      "persistent": true
    }
  }
}
